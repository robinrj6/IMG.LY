{
  "name": "@imgly-labs/imgly-website-components",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build:esm": "tsc && vite build",
    "build:umd": "tsc && vite build -c vite.config.umd.js",
    "storybook": "storybook dev -p 6006 --no-open -h dev.img.ly",
    "storybook:build": "storybook build -o docs",
    "lint:css": "stylelint '**/*.scss'",
    "lint:ts": "eslint --ext .ts,.tsx .",
    "test:e2e:umd": "npx playwright test -c ./playwright.config.umd.ts",
    "test:e2e:esm": "npx playwright test -c ./playwright.config.esm.ts",
    "test:start:umd": "serve -s examples/vanilla-js/ -p 3000",
    "test:start:esm": "cd examples/react-js && yarn && yarn build && serve -s build -p 3000",
    "ci": "tsc && vite build && vite build -c vite.config.umd.js --emptyOutDir false"
  },
  "dependencies": {
    "@ariakit/react": "^0.2.17",
    "@types/object-hash": "^3.0.2",
    "classnames": "^2.3.2",
    "object-hash": "^3.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@eslint/eslintrc": "^2.0.3",
    "@playwright/test": "^1.36.1",
    "@storybook/addon-actions": "^7.0.24",
    "@storybook/addon-essentials": "^7.0.24",
    "@storybook/addon-interactions": "^7.0.24",
    "@storybook/addon-links": "^7.0.24",
    "@storybook/blocks": "^7.0.24",
    "@storybook/react": "^7.0.24",
    "@storybook/react-vite": "^7.0.24",
    "@storybook/testing-library": "^0.2.0",
    "@stylelint/eslint-config": "^12.0.0",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^4.0.1",
    "babel-loader": "^9.1.2",
    "copy-to-clipboard": "^3.3.3",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-mdx": "^2.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.12",
    "postcss": "8.4.24",
    "postcss-merge-longhand": "^6.0.0",
    "postcss-merge-rules": "^6.0.1",
    "postcss-unique-selectors": "^6.0.0",
    "prettier": "^2.8.8",
    "sass": "^1.63.6",
    "serve": "^14.2.0",
    "storybook": "^7.0.24",
    "stylelint": "^15.9.0",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-prettier": "^3.0.0",
    "typescript": "^5.1.6",
    "vite": "^4.3.9",
    "vite-plugin-dts": "^2.3.0",
    "vite-plugin-static-copy": "^0.17.0",
    "vite-plugin-svgr": "^3.2.0"
  }
}
